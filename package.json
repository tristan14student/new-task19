{
    "name": "react-express-heroku-app",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "client": "cd client && npm start",
        "dev": "concurrently --kill-others-on-fail \"npm run start\" \"npm run client\""
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.17.3",
        "helmet": "3.21.1",
        "http-errors": "~1.6.3",
        "jade": "~1.11.0",
        "morgan": "~1.9.1",
        "node-fetch": "2.6.0"
    },
    "devDependencies": {
        "concurrently": "4.1.2"
    }
}
